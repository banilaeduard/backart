{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Warning",
      "Host.Results": "Warning",
      "Function": "Warning",
      "DurableTask": "Warning",
      "Activity": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request",
        "maxTelemetryItemsPerSecond": 5
      },
      "enableLiveMetricsFilters": true,
      "enableDependencyTracking": false
    }
  }
}